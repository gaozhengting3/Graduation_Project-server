{
	"compilerOptions": {
		"resolveJsonModule": true,
		"target": "es2016",
		"module": "commonjs",
		// "rootDir": "./src",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@data/*": ["./data/*"],
			"@private/*": ["./private/*"],
			"@public/*": ["./public/*"]
		},
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,

		"strict": true,
		"skipLibCheck": true
	},
	"include": ["**/*.ts" ,"./types/**.ts"],
	// "exclude": ["./dist"]
}
